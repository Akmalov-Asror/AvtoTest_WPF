<Page x:Class="Avtoauto.Pages.Tickets"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Avtoauto.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Tickets">

    <Grid>
        <ScrollViewer Margin="0 50 0 50">
            <StackPanel Name="TicketButtonPanel"></StackPanel>
        </ScrollViewer> 
        <Button Click="Close_Button" Content="Close" Height="60" Width="100" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="20"></Button>
    </Grid>
</Page>
